    "pino": "^9.3.2",
    "postgres": "^3.4.4",
    "slugify": "^1.6.6",
    "supertest": "^7.1.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.x.x",
    "@types/pg": "^8.11.6",
